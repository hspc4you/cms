package com.jsz.peini.bean;

public class WxFestivalActivityUser {
    private Integer id;

    private String wxUserId;

    private Integer activityId;

    private String activityTime;

    private Integer isAward;
    
    private Integer awardChance;

    private Integer wxIsShare;

    private String wxShareTime;

    private Integer todayCount;
    
    private Integer shareSum;
    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getWxUserId() {
        return wxUserId;
    }

    public void setWxUserId(String wxUserId) {
        this.wxUserId = wxUserId;
    }

    public Integer getActivityId() {
        return activityId;
    }

    public void setActivityId(Integer activityId) {
        this.activityId = activityId;
    }


    public Integer getAwardChance() {
        return awardChance;
    }

    public void setAwardChance(Integer awardChance) {
        this.awardChance = awardChance;
    }

    public Integer getWxIsShare() {
        return wxIsShare;
    }

    public void setWxIsShare(Integer wxIsShare) {
        this.wxIsShare = wxIsShare;
    }

	public String getActivityTime() {
		return activityTime;
	}

	public void setActivityTime(String activityTime) {
		this.activityTime = activityTime;
	}

	public String getWxShareTime() {
		return wxShareTime;
	}

	public void setWxShareTime(String wxShareTime) {
		this.wxShareTime = wxShareTime;
	}

	public Integer getIsAward() {
		return isAward;
	}

	public void setIsAward(Integer isAward) {
		this.isAward = isAward;
	}

	public Integer getTodayCount() {
		return todayCount;
	}

	public void setTodayCount(Integer todayCount) {
		this.todayCount = todayCount;
	}

	public Integer getShareSum() {
		return shareSum;
	}

	public void setShareSum(Integer shareSum) {
		this.shareSum = shareSum;
	}

}