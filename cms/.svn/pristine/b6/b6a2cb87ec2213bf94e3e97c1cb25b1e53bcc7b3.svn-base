			)c WHERE c.userId = #{userId}
	</select>
	
	<update id="updateSelectiveByPrimaryKey" parameterType="UserInfo">
		update user_info
	    <set >
	      <if test="reputation != null and reputation != ''" >
	        reputation = #{reputation},
	      </if>
	      <if test="accCode != null" >
	        acc_code = #{accCode,jdbcType=VARCHAR},
	      </if>
	    </set>
	    where id = #{id,jdbcType=VARCHAR}
	</update>
	<select id="selectUserAccCodeByUserId" parameterType="String" resultType="String">
		SELECT
			IFNULL(a.acc_code,a.user_phone) AS accCode
		FROM
			user_info a
		WHERE
			a.id =#{userId}
	</select>
	<!-- 获取用户的登录信息、基本信息及账户信息 -->
	<select id="getUserInfoAndLoginAccountInfo" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		SELECT	
			user_info.id as userId,
			ifnull(user_info.nickname,'') as nickName,
			user_info.age as age,
			user_info.sex as sex,
			ifnull(user_info.gold_list,0) as goldList,
			ifnull(user_info.buy_list,0) as buyList,
			ifnull(user_info.integrity_list,0) as integrityList,
			user_login.user_name as mobileNo,
			user_account.id as userAccountId, 
			ifnull(user_account.gold,0) as gold,
			ifnull(user_account.score,0) as score	
		FROM
			user_account,
			user_info,
			user_login
		WHERE
			user_account.user_id = user_info.id
			and user_info.user_login_id = user_login.id
			<if test="userId != null and userId != ''" >
				and user_info.id = #{userId}
			</if>
	</select>
	
	<!-- 根据手机号列表获取用户信息 -->
	<select id="getUserInfoByMobiles" resultMap="BaseResultMap">
		SELECT	
			<include refid="Base_Column_List" />
		    ,
		    <include refid="Blob_Column_List" />
    	from user_info
		WHERE
			user_login_id IN (
				SELECT
					id
				FROM
					user_login
				WHERE
					user_name IN 
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
				#{item}
				</foreach>
			)
	</select>
	
	<!-- 将基本用户信息放入redis -->
	<select id="getUserInfoForRedis" resultType="java.util.HashMap">
		SELECT
			a.id as userId,
			b.id as acountId,
			ifnull(a.idCardNo,'') as idCardNo
		FROM
			user_info a,
			user_account b
		where a.id = b.user_id
	</select>
</mapper>